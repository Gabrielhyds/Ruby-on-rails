<div id="<%= dom_id coin %>">
  <p>
    <strong>Descrição:</strong>
    <%= coin.description %>
  </p>

  <p>
    <strong>Sigla:</strong>
    <%= coin.acronym %>
  </p>

  <p>
    <strong>Url da imagem:</strong>
    <%= coin.url_image %>
    <%#= image_tag @coin.url_image, size: "25x25"%>
  </p>

</div>
